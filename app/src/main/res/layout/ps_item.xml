<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvPsKod"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:text="123456"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvPsIrsz"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="1234"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvPsKod"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/tvPsCim"
        android:layout_width="201dp"
        android:layout_height="18dp"
        android:layout_marginEnd="24dp"
        android:text="Város utca 1 "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvPsIrsz"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/tvPsNev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginStart="24dp"
        android:text="Kukuruzsnyák Rémus"
        app:layout_constraintStart_toEndOf="@+id/tvPsKod"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>